{
  "name": "next-graphql-react",
  "version": "6.0.1",
  "description": "Next.js config and App decorators for graphql-react, enabling server side rendered GraphQL operations.",
  "license": "MIT",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "https://jaydenseric.com"
  },
  "repository": "github:jaydenseric/next-graphql-react",
  "homepage": "https://github.com/jaydenseric/next-graphql-react#readme",
  "bugs": "https://github.com/jaydenseric/next-graphql-react/issues",
  "funding": "https://github.com/sponsors/jaydenseric",
  "keywords": [
    "next",
    "graphql",
    "react",
    "esm",
    "mjs"
  ],
  "files": [
    "universal"
  ],
  "main": "universal",
  "exports": {
    ".": {
      "import": "./universal/index.mjs",
      "require": "./universal/index.js"
    },
    "./universal": {
      "import": "./universal/index.mjs",
      "require": "./universal/index.js"
    },
    "./universal/": "./universal/",
    "./package": "./package.json",
    "./package.json": "./package.json"
  },
  "engines": {
    "node": "10 - 12 || >= 13.7"
  },
  "browserslist": "Node 10 - 12, Node >= 13.7, > 0.5%, not OperaMini all, not dead",
  "peerDependencies": {
    "graphql-react": "8.3.0 - 10",
    "next": "^9.0.3",
    "react": "^16.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "http-link-header": "^1.0.2",
    "object-assign": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-require-extensions": "^2.0.1",
    "babel-plugin-transform-runtime-file-extensions": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-config-env": "^13.0.5",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^24.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "graphql-react": "^10.0.0",
    "jsdoc-md": "^5.0.1",
    "next": "^9.3.6",
    "prettier": "^2.0.5",
    "react": "^16.13.1"
  },
  "scripts": {
    "prepare": "npm run prepare:clean && npm run prepare:esm && npm run prepare:cjs && npm run prepare:jsdoc && npm run prepare:prettier",
    "prepare:clean": "rm -rf universal",
    "prepare:esm": "PREPARE_MODULE_TYPE=esm babel src -d . --keep-file-extension",
    "prepare:cjs": "PREPARE_MODULE_TYPE=cjs babel src -d .",
    "prepare:jsdoc": "jsdoc-md",
    "prepare:prettier": "prettier --write universal readme.md",
    "test": "npm run test:eslint && npm run test:prettier",
    "test:eslint": "eslint --ext mjs,js .",
    "test:prettier": "prettier -c .",
    "prepublishOnly": "npm test"
  }
}
